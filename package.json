{
  "name": "vprivacy-holder",
  "version": "0.0.1",
  "private": true,
  "description": "Vue.js vprivacy-holder component",
  "author": {
    "name": "ihavecoke",
    "email": "ihavecoke@163.com"
  },
  "repository": "https://github.com/ihavecoke/vprivacy-holder",
  "bugs": "https://github.com/ihavecoke/vprivacy-holder/issues",
  "keywords": [
    "Privacy",
    "Vuejs",
    "vprivacy-holder"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:npm": "vue-cli-service build --target lib --name VPrivacyHolder src/components/VPrivacyHolder.vue",
    "publish:npm": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/VPrivacyHolder.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/VPrivacyHolder.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/VPrivacyHolder.min.js"
  },
  "main": "dist/VPrivacyHolder.umd.js",
  "module": "dist/VPrivacyHolder.esm.js",
  "unpkg": "dist/VPrivacyHolder.umd.js",
  "dependencies": {
    "core-js": "^3.6.4",
    "privacy-holder": "^0.0.7",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "rollup": "^1.17.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup-plugin-vue": "^5.0.1",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
